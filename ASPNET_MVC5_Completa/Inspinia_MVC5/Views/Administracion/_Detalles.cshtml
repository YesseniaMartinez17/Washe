@model Inspinia_MVC5.Models.tbCategoriaServicios


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <h3>Informacion de auditoria</h3>
            <div class="ibox-content">

                <p><b>Registrado la fecha y hora: </b> @Model.cserv_FechaCrea</p>
                <p><b>Registrado por el usuario: </b>@Model.tbUsuarios.usu_NombreDeUsuario</p>

                @if (Model.cserv_UsuarioModifica != null)
                {
                    <p><b>Modificado por: </b>@Model.tbUsuarios1.usu_NombreDeUsuario</p>
                    <p><b>Fecha modificación: </b>@Model.cserv_FechaModifica</p>
                    if (Model.cserv_Estado == false)
                    {
                    }
                        <p><b>Razón de inactivación: </b>@Model.cserv_RazonInactivo</p>
                }

            </div>

            <h3>Servicios pertenecientes a esta categoría: </h3>
            <div class="ibox-content">
                @{int cont = 0; }
                @foreach (var obj in Model.tbServicios)
                {
                    cont++;
                    <p>
                        @cont
                        .
                        @obj.serv_Descripcion
                    </p>
                }

            </div>
        </div>
    </div>
</div>