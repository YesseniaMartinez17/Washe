@model IEnumerable<Inspinia_MVC5.Models.tbPedidosDetalle>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles del pedido</h5>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pdet_PrecioActual)
        </th>
        <th>
            Servicio de limpieza solicitado
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.pdet_PrecioActual)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbServicios.serv_Titulo)
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



